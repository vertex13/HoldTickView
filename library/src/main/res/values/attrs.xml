<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="HoldCheckBox">
        <!-- A color in the checked state. -->
        <attr name="checkedColor" format="color" />
        <!-- A color in the unchecked state. -->
        <attr name="uncheckedColor" format="color" />
        <!-- Switching time in milliseconds. -->
        <attr name="switchingTime" format="integer" />
    </declare-styleable>

</resources>
